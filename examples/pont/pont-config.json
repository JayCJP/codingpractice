{
  "origins": [
    {
      "name": "exhibit",
      "originUrl": ""
    },
    {
      "name": "livebapi",
      "originUrl": ""
    }
  ],
  "templateType": "fetch",
  "templatePath": "./pontConfig/pontTemplate",
  "transformPath": "./pontConfig/transformPath",
  "outDir": "./src/api",
  "spiltApiLock": false,
  "usingMultipleOrigins": true,
  "mocks": {
    "enable": false
  },
  "prettierConfig": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "proseWrap": "never",
    "printWidth": 125,
    "htmlWhitespaceSensitivity": "ignore",
    "endOfLine": "auto"
  }
}